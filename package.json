{
  "name": "next-tailwind-ui",
  "version": "1.0.0",
  "description": "Complete Next template for repid development",
  "repository": "git@github.com:rayriffy/next-tailwind-ui.git",
  "author": "Phumrapee Limpianchop <contact@rayriffy.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "yarn --silent dev:debugger",
    "dev:normal": "next",
    "dev:debugger": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "cron:hooker": "ts-node --project tsconfig.tools.json scripts/hooker.ts",
    "cron:getDBLatestUpdate": "ts-node --project tsconfig.tools.json scripts/getDBLatestUpdate.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@next/bundle-analyzer": "^10.2.3",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@zeit/next-workers": "^1.0.0",
    "axios": "^0.21.0",
    "dayjs": "^1.10.5",
    "ical-generator": "^2.2.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.3",
    "next": "^10.2.3",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.5",
    "next-plugin-preact": "^3.0.6",
    "postcss": "^8.3.4",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "react-transition-group": "^4.4.2",
    "react-twitter-embed": "^3.0.3",
    "sitemap": "^7.0.0",
    "storeon": "^3.1.1",
    "swr": "^0.5.6",
    "tailwindcss": "^2.1.4",
    "tailwindcss-filters": "^3.0.0",
    "web-api-hooks": "^3.0.2",
    "webpack": "4.46.0",
    "worker-loader": "^3.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.170",
    "@types/markdown-to-jsx": "^7.0.1",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-transition-group": "^4.4.1",
    "@types/wicg-mediasession": "^1.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
