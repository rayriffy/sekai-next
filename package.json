{
  "name": "next-tailwind-ui",
  "version": "1.0.0",
  "description": "Complete Next template for repid development",
  "repository": "git@github.com:rayriffy/next-tailwind-ui.git",
  "author": "Phumrapee Limpianchop <contact@rayriffy.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "yarn --silent dev:debugger",
    "dev:normal": "next",
    "dev:debugger": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "cron:hooker": "ts-node --project tsconfig.tools.json scripts/hooker.ts",
    "cron:getDBLatestUpdate": "ts-node --project tsconfig.tools.json scripts/getDBLatestUpdate.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.4",
    "@next/bundle-analyzer": "^11.1.2",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "axios": "^0.21.4",
    "chrome-aws-lambda": "^10.1.0",
    "dayjs": "^1.10.7",
    "ical-generator": "^3.0.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.3",
    "moize": "^6.1.0",
    "next": "^11.1.2",
    "next-compose-plugins": "^2.2.1",
    "next-offline": "^5.0.5",
    "next-plugin-preact": "^3.0.6",
    "postcss": "^8.3.8",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "puppeteer-core": "^10.4.0",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "sitemap": "^7.0.0",
    "storeon": "^3.1.1",
    "tailwindcss": "^2.2.16",
    "tailwindcss-filters": "^3.0.0",
    "web-api-hooks": "^3.0.2",
    "webpack": "^5.54.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/preset-env": "^7.15.6",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.175",
    "@types/markdown-to-jsx": "^7.0.1",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.24",
    "@types/wicg-mediasession": "^1.1.3",
    "babel-plugin-lodash": "^3.3.4",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
