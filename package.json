{
  "name": "next-tailwind-ui",
  "version": "1.0.0",
  "description": "Complete Next template for repid development",
  "repository": "git@github.com:rayriffy/next-tailwind-ui.git",
  "author": "Phumrapee Limpianchop <contact@rayriffy.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "yarn --silent dev:debugger",
    "dev:normal": "next",
    "dev:debugger": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "cron:hooker": "ts-node --project tsconfig.tools.json scripts/hooker.ts",
    "cron:getDBLatestUpdate": "ts-node --project tsconfig.tools.json scripts/getDBLatestUpdate.ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^0.3.1",
    "@next/bundle-analyzer": "^10.0.9",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "@zeit/next-workers": "^1.0.0",
    "axios": "^0.21.0",
    "dayjs": "^1.10.4",
    "ical-generator": "^1.15.4",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.0.1",
    "next": "^10.0.9",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.3",
    "next-plugin-preact": "^3.0.3",
    "postcss": "^8.2.8",
    "preact": "^10.5.12",
    "preact-render-to-string": "^5.1.16",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "react-transition-group": "^4.4.1",
    "react-twitter-embed": "^3.0.3",
    "sitemap": "^6.4.0",
    "storeon": "^3.1.1",
    "swr": "^0.4.2",
    "tailwindcss": "^2.0.4",
    "tailwindcss-filters": "^3.0.0",
    "web-api-hooks": "^3.0.2",
    "webpack": "4.46.0",
    "worker-loader": "^3.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.168",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-transition-group": "^4.4.1",
    "@types/wicg-mediasession": "^1.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.3"
  }
}
